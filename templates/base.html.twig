<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<meta
		name="viewport" content="width=device-width, initial-scale=1.0">

		<!-- Latest compiled and minified CSS -->
		<link
		href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">

		<!-- Latest compiled JavaScript -->
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>

		<!--========== ICONS ==========-->
		<link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'> {% block css %}{% endblock %}{% block link %}{% endblock %}

		<!--========== CSS ==========-->
		<link rel="stylesheet" href="{{ asset('assets/css/styles.css') }}">
		 {% block stylesheets %}
		   {{ encore_entry_link_tags('app') }}
		{% endblock %}

		<!--========== FAVICON ==========-->
		<link
		rel="shortcut icon" type="image/png" href="{{ asset('assets/img/favicon.png') }}">

		<!-- Global site tag (gtag.js) - Google Analytics -->
		<script async src="https://www.googletagmanager.com/gtag/js?id=G-FY5BPYYG3E"></script>

		<script>
			window.dataLayer = window.dataLayer || [];
function gtag() {
dataLayer.push(arguments);
}
gtag('js', new Date());
gtag('config', 'G-FY5BPYYG3E');
		</script>

		<title>
			{% block title %}Génération Boomerang{% endblock %}
		</title>
	</head>

	<body data-turbo = 'false'>
		<!--========== CONTENTS ==========-->

		{% block content %}{% endblock %}
		<main class="main">
			{% include "_partials/_nav.html.twig" %}
			{# {% include "_partials/_navLeft.html.twig" %} #}
		</main>
		{% include "_partials/_footer.html.twig" %}

		<!--========== MODAL ==========-->
		{% block modal %}{% endblock %}
		
		<!--========== Block Javascript ==========-->
		{% block JavaScript %}{{ encore_entry_script_tags('app') }}{% endblock %}

		<!--========== MAIN JS ==========-->
		<script src="{{ asset('assets/js/main.js') }}"></script>
	</body>
</html>
